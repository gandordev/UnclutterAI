{
  "manifest_version": 3,
  "version": "1.0.2",
  "name": "UnclutterAI",
  "description": "A lightweight browser extension that blocks subscription reminders on AI platforms.",
  "author": "Gabriel Ramos LÃ³pez",
  "content_scripts": [
    {
      "matches": [
        "https://*.ai/*",
        "https://chatgpt.com/*",
        "https://chat.openai.com/*",
        "https://gemini.google.com/*"
      ],
      "js": [
        "unclutter.js"
      ]
    }
  ],
  "icons": {
    "32": "icon/icon.png",
    "64": "icon/icon@2x.png"
  }
}